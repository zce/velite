    "@types/mdast": "^4.0.3",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.10.4",
    "@zce/prettier-config": "^0.2.0",
    "chokidar": "^3.5.3",
    "fast-glob": "^3.3.2",
